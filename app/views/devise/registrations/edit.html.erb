<div class="delete-account container">
  <h2>Delete Account?</h2>
  
  <%= button_to "Cancel my account", registration_path(resource_name),
      data: { confirm: "Are you sure?" },
      method: :delete,
      class: 'btn btn-danger' 
  %>
  
  <%= button_to "Back", :back, class: 'btn btn-primary' %>
</div>